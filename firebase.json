{
  "functions": {
    "source": "functions"
  },
  "hosting": {
    "public": "public",
    "rewrites": [ {
      "source": "/analyze",
      "function": "analyze"
    } ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**",
      "**/keys/**"
    ]
  },
  "emulators": {
    "functions": {
      "port": 5001
    },
    "hosting": {
      "port": 5000
    },
    "ui": {
      "enabled": true
    }
  }
}
